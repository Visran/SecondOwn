{
  "name": "Disable MPR",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Disable Merge pull request Button when PR requires Code Review on GitHub.",
  
  "permissions": [
    "tabs", "https://github.com/*/*/pull/*"
  ],
  "content_scripts": [
    {
      "matches": [ "https://github.com/*/*/pull/*" ],
      "js": [ "myscript.js" ]
    }
  ]
}
